{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQLConnection": "Server=localhost;Port=5432;Database=geeks-auth;Username=postgres;Password=YOUR_PASSWORD_HERE;"
  },
  "JWT": {
    "Key": "YourSuperSecretKeyHere12345678901234567890",
    "Issuer": "YourApp",
    "Audience": "YourAppUsers"
  },
  "GoogleCloud": {
    "ProjectId": "YOUR_GOOGLE_CLOUD_PROJECT_ID",
    "Location": "us-central1",
    "Model": "gemini-1.5-flash",
    "MaxTokens": 500,
    "Temperature": 0.7,
    "CredentialsPath": "credentials/service-account.json",
    "ApiKey": "YOUR_GEMINI_API_KEY_HERE"
  }
}
